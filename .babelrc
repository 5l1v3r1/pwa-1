{
  "presets": [
    ["env", {
      "targets": {
        "uglify": true
      },
      "loose": true,
      "modules": false,
      "useBuiltIns": "entry"
    }],
    "react",
    "stage-1"
  ],
  "plugins": [
    "react-loadable/babel"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "react",
        "stage-1"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
